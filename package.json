{
  "type": "module",
  "name": "parallel-realms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:backend": "ts-node backend.ts",
    "dev": "concurrently \"npm run start\" \"npm run start:backend\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:Parallel-Realms": "node dist/Parallel-Realms/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.0",
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/platform-server": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@angular/ssr": "^21.1.2",
    "@prisma/client": "^6.19.2",
    "@turf/turf": "^7.3.3",
    "@types/leaflet": "^1.9.21",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "prisma": "^6.19.2",
    "rxjs": "~7.8.0",
    "stripe": "^20.3.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
